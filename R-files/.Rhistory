alpha<- 0.05
n <- 64
varianza <- 625
media <- 1012
cuantil<- qnorm(1 - alpha/2)
lim_inf<-media - cuantil * sqrt(varianza) / sqrt(n)
lim_inf
lim_sup<- media + cuantil * sqrt(varianza) / sqrt(n)
lim_sup
cbind(lim_inf,lim_sup)
alpha<- 0.05
n <- 64
varianza <- 625
media <- 1012
cuantil<- qnorm(alpha/2)
lim_inf<-media - cuantil * sqrt(varianza) / sqrt(n)
lim_inf
lim_sup<- media + cuantil * sqrt(varianza) / sqrt(n)
lim_sup
cbind(lim_inf,lim_sup)
